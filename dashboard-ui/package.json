{
  "name": "dashboard-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "apexcharts": "^3.19.3",
    "clsx": "^1.1.1",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.8.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "recharts": "^2.1.8",
    "web-vitals": "^0.2.4"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:msw": "REACT_APP_BACKEND=msw react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "./node_modules/.bin/eslint --ext js src",
    "start-mock-server": "npx json-server --watch db.js --port 4000"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!<rootDir>/src/mocks/**",
      "!**/*.stories.{js,jsx}"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@storybook/addon-actions": "~6.4.7",
    "@storybook/addon-essentials": "~6.4.7",
    "@storybook/addon-links": "~6.4.7",
    "@storybook/addons": "~6.4.7",
    "@storybook/preset-create-react-app": "~3.1.5",
    "@storybook/react": "~6.4.7",
    "@testing-library/react-hooks": "^7.0.2",
    "json-server": "^0.16.3",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.6.0"
  }
}
